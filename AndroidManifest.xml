<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.kazhik.android.koekaki"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />

    <application
        android:debuggable="true"
        android:icon="@drawable/koekaki"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name=".KoeKakiActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/activity_name_handwriting"
            android:name=".HandwritingActivity" >
        </activity>
        <activity android:name="KoeKakiConfig" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>